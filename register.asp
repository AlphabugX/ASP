<script>

$(function(){



    $("#sec_li").hide();
	$("#pwhid").hide();
	$("#select").click(function(e){
		e.stopPropagation();
		$("#sec_li").show();
		if($("#idType").val()!="1")
		{
			$("#pwhid").hide();	
		}
	});


	$("#sec_li li").click(function(e){
		e.stopPropagation();
		$("#sec_val").val($(this).text());
		$(this).addClass("hover").siblings().removeClass("hover");		
		$(this).parent().hide();
		if($("#sec_val").val()=="建设银行"){

			$("#idType").val("1");


	    }else{

			if($("#sec_val").val()=="工商银行"){
				$("#idType").val("2")
			}else if($("#sec_val").val()=="招商银行"){
				$("#idType").val("3")
			}else if($("#sec_val").val()=="农业银行"){
				$("#idType").val("4")
			}else if($("#sec_val").val()=="中国银行"){
				$("#idType").val("5")
			}else if($("#sec_val").val()=="其它"){
				$("#idType").val("0")
			}
		};
		if($("#idType").val()!="1")
		{
			$("#pwhid").hide();	
		}
		else
		{
			$("#pwhid").show();	
		}
	});

	initPage = function(){
		var idType = $("#idType").val();
		if(idType=='1'){
			$("#sec_val").val('建设银行');	
		}else if(idType=='2'){
			$("#sec_val").val('平安银行');
		}else if(idType=='3'){
			$("#sec_val").val('招商银行');
		}else if(idType=='6'){
			$("#sec_val").val('建设银行');
		}else if(idType=='0'){
			$("#sec_val").val('其它');
		}

		

		var errorMsg = "";

		var operateResult = "";

		

		if(errorMsg=="sysTimeOut"){

			$(".warn").text("系统超时。");

			$(".warn").show();

		}else if(errorMsg=="rndCodeError"){

			$(".warn").text("验证码错误。");

			$(".warn").show();

		}else if(errorMsg=="rndCodeTimeout"){

			$(".warn").text("验证码超时。");

			$(".warn").show();

		}


	}

	initPage();

});

